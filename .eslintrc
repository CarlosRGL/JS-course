{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "node": true,
    "es6": false,
    // # "mocha": true,
    "jquery": true
  },
  "rules": {
    "no-console": 0,
    "indent": ["error", 2],
    "valid-jsdoc": [
      "error",
      {
        "requireReturn": true,
        "requireReturnType": true,
        "requireParamDescription": true,
        "requireReturnDescription": true
      }
    ]
    // "require-jsdoc": [
    //   "error",
    //   {
    //     "require": {
    //       "FunctionDeclaration": true,
    //       "MethodDefinition": true,
    //       "ClassDeclaration": true
    //     }
    //   }
    // ]
  }
}
